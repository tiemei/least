<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="MAIN" class="ch.qos.logback.core.ConsoleAppender">
        <encoder charset="UTF-8">
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5p %c{80} : %m%n</pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="MAIN" />
    </root>

</configuration>